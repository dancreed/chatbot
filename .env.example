# Environment Configuration Example
# Copy this file to .env.local and update with your actual values

# Cloudflare API Configuration (Server-side only)
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_AI_ENDPOINT=https://api.cloudflare.com/client/v4/accounts/your_account_id/ai/run/@cf/meta/llama-2-7b-chat-fp16

# WebSocket Configuration (Client-side - must start with NEXT_PUBLIC_)
NEXT_PUBLIC_WS_URL=wss://your-worker-domain.workers.dev/websocket
# For local development:
# NEXT_PUBLIC_WS_URL=ws://localhost:8787/websocket

# API Base URL (Client-side - optional)
NEXT_PUBLIC_API_BASE_URL=

# Development/Production Environment
NODE_ENV=development